#!/usr/bin/perl

# PODNAME: koha-unimarc-4xx-fix
# ABSTRACT: Fix inconsistencies between mother/child biblio records

use Modern::Perl;
use Pod::Usage;
use Getopt::Long;
use Koha::Contrib::Tamil::Unimarc4xx;


my ($help)  = (0);
GetOptions( 
    'help|h'       => \$help,
);


sub usage {
    pod2usage( -verbose => 2 );
    exit;
} 


usage() if $help;          

my $dumper = Koha::Contrib::Tamil::Unimarc4xx->new(
    action => \@ARGV
);
$dumper->run();


=head1 NAME

koha-unimarc-4xx-fix - Fix inconsitencies between mother/child biblio records.

